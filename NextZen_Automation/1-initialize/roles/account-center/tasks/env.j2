AUTHENTIK_IMAGE={{ acc_image }}
AUTHENTIK_TAG={{ acc_tag }}
SERVER_CONTAINER={{ acc_server_container }}
WORKER_CONTAINER={{ acc_worker_container }}
AUTHENTIK_SECRET_KEY={{ acc_secret_key }}
# SMTP Host Emails are sent to
AUTHENTIK_EMAIL__HOST={{ acc_email_host }}
AUTHENTIK_EMAIL__PORT={{ acc_email_port }}
# Optionally authenticate (don't add quotation marks to your password)
AUTHENTIK_EMAIL__USERNAME={{ acc_email }}
AUTHENTIK_EMAIL__PASSWORD={{ acc_email_pass }}
# Use StartTLS
AUTHENTIK_EMAIL__USE_TLS={{ acc_email_tls }}
# Use SSL
AUTHENTIK_EMAIL__USE_SSL={{ acc_email_ssl }}
AUTHENTIK_EMAIL__TIMEOUT={{ acc_email_timeout }}
# Email address authentik will send from, should have a correct @domain
AUTHENTIK_EMAIL__FROM={{ acc_email_from }}
# Data storage
AUTHENTIK_MEDIA={{ apps_dest }}/AccountCenter/media
AUTHENTIK_TMP={{ apps_dest }}/AccountCenter/custom_templates
AUTHENTIK_CERTS={{ apps_dest }}/AccountCenter/certs
COMPOSE_PORT_HTTP={{ acc_http_port }}
COMPOSE_PORT_HTTPS={{ acc_https_port }}
# GeoIP
GEOIP_ACCOUNT_ID={{ geoip_accid }}
GEOIP_LICENSE_KEY={{ geoip_key }}
GEOIP_DATA={{ apps_dest }}/AccountCenter/geoip
# Postgres
PG_PASS={{ acc_db_pass }}
PG_DATA={{ apps_dest }}/AccountCenter/database
# Redis
CACHE_DATA={{ apps_dest }}/AccountCenter/redis